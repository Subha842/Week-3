class Node:
    def __init__(self, data):
        self.data = data
        self.next = None

def deleteNode(head, x):
    if x == 1:
        return head.next
    curr = head
    for _ in range(x - 2):
        curr = curr.next
    curr.next = curr.next.next
    return head

def printList(head):
    while head:
        print(head.data, end=" -> " if head.next else "")
        head = head.next
    print()

head = Node(1)
head.next = Node(3)
head.next.next = Node(4)

printList(head)
head = deleteNode(head, 3)
printList(head)
